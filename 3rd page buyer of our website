       function GFG_Fun() {
        for (let i = 0; i < elmts.length; i++) {
                let optn = elmts[i];
                let el = document.createElement("option");
                el.textContent = optn;
                el.value = optn;
                select.appendChild(el);
            }
        }
        

     
    
