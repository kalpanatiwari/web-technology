<script>
let login = document.getElementById("nav-login");
let signup = document.getElementById("nav-signup");
let container1 = document.getElementsByClassName("container1")[0];
let container2 = document.getElementsByClassName("container1_signup")[0];
let box = document.getElementsByClassName("box1")[0];
let box1 = document.getElementsByClassName("box1_signup")[0];
let wel = document.getElementsByClassName("wel")[0];
let come = document.getElementsByClassName("come")[0];
let email_signup = document.getElementById("email_signup");
login.onclick = () => {
container1.classList.toggle("flex");
container1.style.visibility = "visible";
box.classList.add("pos");
wel.style.display = "none";
come.style.display = "none";
};
let eye = document.getElementById("eye");
let password = document.getElementById("password");
eye.onclick = function () {
if (password.type == "password") {
password.type = "text";
eye.src = "eyeclose.png";
} else {
password.type = "password";
eye.src = "eyeopen.png";
}
};
signup.onclick = () => {
container2.classList.toggle("flex");
container2.style.visibility = "visible";
box1.classList.add("pos1");
wel.style.display = "none";
come.style.display = "none";
};
let eye_signup = document.getElementById("eye_signup");
let password_signup = document.getElementById("password_signup");
eye_signup.onclick = function () {
if (password_signup.type == "password") {
password_signup.type = "text";
eye_signup.src = "eyeclose.png";
} else {
password_signup.type = "password";
eye_signup.src = "eyeopen.png";
}
};

let google = document.getElementsByClassName("google")[0];
google.onclick = () => {
document.getElementById("UserName_signup").removeAttribute("required");
password_signup.removeAttribute("required");
email_signup.removeAttribute("required");
};

let service = document.getElementById("services");
let service1 = document.getElementsByClassName("service")[0];
service.onclick = () => {
service1.classList.toggle("none");
};

